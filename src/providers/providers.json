      "scope": "r_liteprofile r_emailaddress",
      "responseType": "code",
      "responseMode": "query",
      "documentationUrl": "https://docs.microsoft.com/en-us/linkedin/shared/authentication/",
      "setupInstructions": "1. Create LinkedIn app\n2. Verify your app\n3. Request OAuth 2.0 scopes\n4. Add redirect URLs\n5. Copy Client ID and Secret",
      "requiredFields": ["clientId", "clientSecret", "redirectUri"]
